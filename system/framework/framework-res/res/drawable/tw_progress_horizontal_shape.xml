<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <shape>
            <corners android:radius="4.659973dip" />
            <gradient android:startColor="#55989898" android:endColor="#55686868" android:angle="270.0" android:centerColor="#55686868" />
        </shape>
    </item>
    <item android:id="@id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="4.659973dip" />
                <gradient android:startColor="#8053ff53" android:endColor="#8002d402" android:angle="270.0" android:centerColor="#8003d503" />
            </shape>
        </scale>
    </item>
    <item android:id="@id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="4.659973dip" />
                <gradient android:startColor="#ff53ff53" android:endColor="#ff02d402" android:angle="270.0" android:centerColor="#ff03d503" />
            </shape>
        </scale>
    </item>
</layer-list>
